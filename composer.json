{
    "name": "lampdev/guzzle-tor",
    "type": "library",
    "description": "This Guzzle middleware allows to use Tor client as a proxy",
    "keywords": ["Guzzle", "Tor", "Onion"],
    "homepage": "http://github.com/lampdev/guzzle-tor",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Prokhorov",
            "email": "alexey.p@l96.ru"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "GuzzleTor\\": "src/"
        }
    },
    "autoload-dev": {
